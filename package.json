{
  "name": "n8n-backend",
  "version": "0.1.0",
  "private": true,
  "description": "Secure ingress and orchestration backend for n8n workflows",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "start:worker": "node dist/workers/index.js",
    "test": "jest",
    "migrate:up": "migrate-mongo up",
    "migrate:down": "migrate-mongo down",
    "migrate:status": "migrate-mongo status",
    "migrate:create": "migrate-mongo create",
    "docker:build": "docker build -t n8n-backend:latest .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@sendgrid/mail": "^8.1.0",
    "@slack/web-api": "^7.3.1",
    "arg": "^5.0.2",
    "argon2": "^0.41.1",
    "axios": "^1.7.7",
    "body-parser": "^1.20.2",
    "bullmq": "^5.12.0",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.4.1",
    "ipaddr.js": "^2.1.0",
    "jsonwebtoken": "^9.0.2",
    "libphonenumber-js": "^1.11.10",
    "mongodb": "^6.7.0",
    "mustache": "^4.2.0",
    "prom-client": "^15.1.3",
    "twilio": "^5.3.3",
    "uuid": "^9.0.1",
    "validator": "^13.11.0",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mustache": "^4.2.6",
    "@types/node": "^20.12.14",
    "@types/uuid": "^10.0.0",
    "@types/validator": "^13.15.10",
    "jest": "^29.7.0",
    "migrate-mongo": "^11.0.0",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  }
}
